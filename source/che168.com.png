                            var ev = document.createEvent('MouseEvents');
                            ev.initEvent('click', false, true);
                            curli[0].firstChild.dispatchEvent(ev);
                        }
                    }
                    else {
                        onSearch();
                    }
                    return;
                case 38:
                    if (curli != null) {
                        index--;
                        curli.removeClass('current');
                    }
                    else {
                        index = shistory_list.length - 1;
