                    curli[0].click();
                } else if (document.createEvent) {
                    var ev = document.createEvent('MouseEvents');
                    ev.initEvent('click', false, true);
                    curli[0].dispatchEvent(ev);
                }
                return;
                // 向上
            case 38:
                if (curli != null) {
                    index--;
                    curli.removeClass('current');
                }
                else {
                    index = liCount - 1;
                }
                if (index < 0)
                    index = liCount - 1;
                curli = liArr.eq(index);
                curli.addClass('current'); 
                return;
