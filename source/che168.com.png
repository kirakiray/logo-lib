    function searchCityEvt(e) {
        var id = "txtCity";
        var searchCity = $("#"+id);
        
        if (searchCity.val() == document.getElementById(id).defaultTip)
        { searchCity.val('');}

        //处理键盘的“上”、“下”、“回车”键
        e = window.event || e;
        var keyCode = e.keyCode || e.which || e.charCode; 
