    $(function () {
      // Click event handler for accordion titles
      $('.acc__title').click(function (j) {
        var dropDown = $(this).closest('.acc__card').find('.acc__panel');
        var accordion = $(this).closest('.acc');
        // Stop any ongoing animations and close other panels
        accordion.find('.acc__panel').not(dropDown).stop(true, true).slideUp(500);
        // Toggle active class for the clicked title
        if ($(this).hasClass('active')) {
          $(this).removeClass('active');
        } else {
          accordion.find('.acc__title.active').removeClass('active');
          $(this).addClass('active');
        }
        // Toggle the panel
        dropDown.stop(true, true).slideToggle(500);
        j.preventDefault();
      });
      // Initialize the first item as open
      var firstTitle = $('.acc__title').first();
      var firstDropDown = firstTitle.closest('.acc__card').find('.acc__panel');

      firstTitle.addClass('active');
      firstDropDown.show(); // Use .show() for immediate visibility or .slideDown(300) for animation
    });
    $(document).ready(function(){
      $('.reviews-masonary-content').masonry({
        itemSelector: '.rmc-grid',
        columnWidth: 330,
        gutter: 20
      });
      const slider = $(".pageflow-testimonials-slider");
      let isScrolling = false;
      slider.on('wheel', function(e) {
        const currentSlide = $(this).slick('slickCurrentSlide');
        const totalSlides = $(this).slick('getSlick').slideCount;
        // Check for horizontal scrolling
        if (Math.abs(e.originalEvent.deltaX) > Math.abs(e.originalEvent.deltaY)) {
          e.preventDefault(); // Prevent page scrolling
          if (e.originalEvent.deltaX < 0) {
            // Scrolling left
            if (currentSlide > 0) {
              if (!isScrolling) {
                isScrolling = true;
                $(this).slick('slickPrev');
                setTimeout(() => { isScrolling = false; }, 500);
              }
            }
          } else if (e.originalEvent.deltaX > 0) {
            // Scrolling right
            if (currentSlide < totalSlides - 1) {
              if (!isScrolling) {
                isScrolling = true;
                $(this).slick('slickNext');
                setTimeout(() => { isScrolling = false; }, 500);
              }
            }
          }
        }
      });
    });
